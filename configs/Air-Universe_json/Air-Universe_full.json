{
  "panel": {
    "type": "sspanel",
    "url": "https://SSPanel.address",
    "key": "SSPanel-Key",
    "node_ids": [1, 2]
  },
  "proxy": {
    "type": "xray",
    "alert_id": 1,
    "auto_generate": true,
    "in_tags": [
      "p0",
      "p1"
    ],
    "api_address": "127.0.0.1",
    "api_port": 10085,
    "log_path": "./v2.log",
    "cert": {
      "cert_path": "/path/to/certificate.crt",
      "key_path": "/path/to/key.key"
    }
  },
  "sync": {
    "interval": 60,
    "fail_delay": 5,
    "timeout": 5
  }
}
